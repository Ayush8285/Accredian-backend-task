datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Referral {
  id            Int    @id @default(autoincrement())
  referrerName  String
  referrerEmail String @unique
  refereeName   String
  refereeEmail  String
  courseName    String
  createdAt     DateTime @default(now())
}
